    <% if(users && users.length){%>
    <%  users.forEach(function(user){ %>
      <% if(user.toBeShown==true){%>
<hr class="h-line-people">
      <div class="row featurette justify-content-center people-description-div">
        <div class="col-sm-2 img-featurette-container">
          <img class="about-img-left featurette-image" src="<%=user.pictureUrl.substring(2)%>" alt="There should be my face">

              <div class="people-social">
                <%if(user.facebook.length){%>
                  <a class="footer-link" href="https://www.instagram.com/<%=user.facebook%>"><img class="img-footer" src="https://upload.wikimedia.org/wikipedia/commons/5/51/Facebook_f_logo_%282019%29.svg" alt=""></a>
