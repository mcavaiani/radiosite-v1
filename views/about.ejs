                <%if(user.instagram.length){%>
                  <a class="footer-link" href="https://www.instagram.com/<%=user.instagram%>"><img class="img-footer" src="https://upload.wikimedia.org/wikipedia/commons/thumb/e/e7/Instagram_logo_2016.svg/1024px-Instagram_logo_2016.svg.png" alt=""></a>
                <%}%>
              </div>



        </div>
        <div class="col-sm-4">
          <h2 class="featurette-heading"><%=user.nickName%></h2>
          <p class="lead"><%=user.description%></p>
        </div>
      </div>

      <% } %>
    <% }) %>
