<!-- Section music -->
<section id="music">
  <div class="myPrograms">
    <h3 class="programs-title">Il blog di <%= title %></h3>
    <!-- <hr class="h-line-programs"> -->
  </div>
  <div class="">
    <% if(posts && posts.length){%>


    <%  posts.forEach(function(post){ %>

      <h1><%=post.title%></h1>
      <p>
      <%=post.content.substring(0, 100) + " ..."%>
      <a href="/<%=stateName%>/posts/<%=post._id%>">Read More</a>
      </p>


      <% }) %>
      <% } %>
  </div>

  <!-- <hr class="h-line-programs"> -->

</section>
